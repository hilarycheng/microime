<?xml version="1.0" encoding="UTF-8"?>
<keyboard name="@string/qwerty">
  <!--
  <row type="numrow">
    <keys size="10%" value="1234567890" />
  </row>
  -->
  <row size="25%">
    <keys size="10%" value="QWERTYUIOP" alt="1234567890" shift="qwertyuiop" />
  </row>
  <row size="25%" offset="5%">
    <keys size="10%" value="ASDFGHJKL" alt="\@#$%&#38;*-+!" shift="asdfghjkl" />
  </row>
  <row size="25%">
    <key size="15%" type="shift" />
    <keys size="10%" value="ZXCVBNM" alt="()&quot;':;/" shift="zxcvbnm"  />
    <key size="15%" type="delete" />
  </row>
  <row size="25%">
    <key size="15%" type="symbols" />
    <key size="10%" type="comma" />
    <key size="50%" type="space" />
    <key size="10%" type="dot" />
    <key size="15%" type="action" />
  </row>
</keyboard>
